<script>
    // Chart variables
    let financialChart, membershipChart;
    const chartColors = {
        loans: '#FFCC80', // Pastel orange for Active Loans
        savings: '#CE93D8', // Pastel purple for Total Savings
        members: '#A5D6A7', // Pastel green for Total Members
        applications: '#90CAF9' // Pastel blue for Pending Applications
    };

    // Function to create or update a single chart
    function createChart(canvasId, datasets, type = 'bar') {
        const ctx = document.getElementById(canvasId).getContext('2d');
        let chartInstance = Chart.getChart(canvasId); // Get existing chart, if any
        
        // Destroy previous chart if it exists
        if (chartInstance) {
            chartInstance.destroy();
        }

        // Prepare chart data with multiple datasets
        const chartData = {
            labels: ['Current Metrics'],
            datasets: datasets.map(dataset => ({
                label: dataset.label,
                data: [dataset.value !== undefined && dataset.value !== null ? Number(dataset.value) : 0],
                backgroundColor: dataset.color,
                borderColor: dataset.color,
                borderWidth: 2,
                tension: 0.4,
                fill: false,
                pointBackgroundColor: dataset.color,
                pointRadius: 3,
                pointHoverRadius: 5
            }))
        };

        // Chart options
        const options = {
            responsive: true,
            maintainAspectRatio: false,
            animation: {
                duration: 800,
                easing: 'easeInOutQuad'
            },
            scales: {
                y: {
                    beginAtZero: true,
                    grid: {
                        drawBorder: false,
                        color: '#f0f0f0'
                    },
                    ticks: {
                        color: '#666',
                        callback: function(value) {
                            if (canvasId === 'financialChart') {
                                return '‚Ç±' + value.toLocaleString('en-PH');
                            }
                            return value.toLocaleString('en-PH', { maximumFractionDigits: 0 });
                        },
                        stepSize: canvasId === 'financialChart' ? Math.max(Math.max(...datasets.map(d => d.value || 0)) / 5, 1000) : 1
                    },
                    suggestedMax: canvasId === 'financialChart' ? Math.max(Math.max(...datasets.map(d => d.value || 0)) * 1.2, 10000) : Math.max(Math.max(...datasets.map(d => d.value || 0)) + 1, 5)
                },
                x: {
                    grid: {
                        display: false
                    },
                    ticks: {
                        color: '#666'
                    }
                }
            },
            plugins: {
                legend: {
                    position: 'top',
                    labels: {
                        color: '#333',
                        font: {
                            family: "'Poppins', sans-serif",
                            size: 12,
                            weight: '500'
                        },
                        padding: 15,
                        boxWidth: 15,
                        usePointStyle: type === 'line'
                    }
                },
                tooltip: {
                    backgroundColor: '#fff',
                    titleColor: '#333',
                    bodyColor: '#666',
                    borderColor: '#eee',
                    borderWidth: 1,
                    padding: 10,
                    boxShadow: '0 4px 15px rgba(0,0,0,0.1)',
                    titleFont: {
                        family: "'Poppins', sans-serif",
                        size: 14,
                        weight: '600'
                    },
                    bodyFont: {
                        family: "'Poppins', sans-serif",
                        size: 12
                    },
                    callbacks: {
                        label: function(context) {
                            let label = context.dataset.label || '';
                            if (label) {
                                label += ': ';
                            }
                            if (context.parsed.y !== undefined) {
                                if (canvasId === 'financialChart') {
                                    label += '‚Ç±' + context.parsed.y.toLocaleString('en-PH');
                                } else {
                                    label += context.parsed.y.toLocaleString('en-PH', { maximumFractionDigits: 0 });
                                }
                            }
                            return label;
                        }
                    }
                }
            },
            elements: {
                line: {
                    tension: 0.4
                },
                bar: {
                    borderRadius: 6,
                    barPercentage: 0.5,
                    categoryPercentage: 0.6
                }
            }
        };

        // Create chart
        return new Chart(ctx, {
            type: type,
            data: chartData,
            options: options
        });
    }

    // Function to initialize charts with specific values
    function initializeCharts(type = 'bar', realData = {}) {
        // Initialize financial chart with 0 values
        financialChart = createChart('financialChart', [
            { label: 'Active Loans', value: 0, color: chartColors.loans },
            { label: 'Total Savings', value: 0, color: chartColors.savings }
        ], type);

        // Initialize membership chart with real or 0 values
        membershipChart = createChart('membershipChart', [
            { label: 'Total Members', value: realData.totalMembers || 0, color: chartColors.members },
            { label: 'Pending Applications', value: realData.pendingApplications || 0, color: chartColors.applications }
        ], type);

        // Transition financial charts to 200,000 after 1 second
        setTimeout(() => {
            if (financialChart) {
                financialChart.data.datasets[0].data = [200000];
                financialChart.data.datasets[1].data = [200000];
                financialChart.options.scales.y.suggestedMax = 200000 * 1.2;
                financialChart.options.scales.y.stepSize = 200000 / 5;
                financialChart.update();
            }

            // Update financial charts with real data after another second
            setTimeout(() => {
                if (financialChart && realData.totalActiveLoans !== undefined && realData.totalSavings !== undefined) {
                    financialChart.data.datasets[0].data = [realData.totalActiveLoans];
                    financialChart.data.datasets[1].data = [realData.totalSavings];
                    financialChart.options.scales.y.suggestedMax = Math.max(Math.max(realData.totalActiveLoans, realData.totalSavings) * 1.2, 10000);
                    financialChart.options.scales.y.stepSize = Math.max(Math.max(realData.totalActiveLoans, realData.totalSavings) / 5, 1000);
                    financialChart.update();
                }
            }, 1000);
        }, 1000);
    }

    // Function to update all charts with real data
    function updateCharts(data, type = 'bar') {
        financialChart = createChart('financialChart', [
            { label: 'Active Loans', value: data.totalActiveLoans || 0, color: chartColors.loans },
            { label: 'Total Savings', value: data.totalSavings || 0, color: chartColors.savings }
        ], type);
        membershipChart = createChart('membershipChart', [
            { label: 'Total Members', value: data.totalMembers || 0, color: chartColors.members },
            { label: 'Pending Applications', value: data.pendingApplications || 0, color: chartColors.applications }
        ], type);
    }

    // Function to update dashboard with data
    function updateDashboard(data) {
        // Update stats cards with real data
        const statCards = document.querySelectorAll('.stat-card');
        
        if (statCards.length >= 5) {
