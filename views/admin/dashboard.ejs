        .rental-icon { background-color: rgba(33, 150, 243, 0.15); color: #2196f3; border-left-color: #2196f3; }
        .savings-icon { background-color: rgba(156, 39, 176, 0.15); color: #9c27b0; border-left-color: #9c27b0; }
        .bayanihan-icon { background-color: rgba(255, 193, 7, 0.15); color: #ffc107; border-left-color: #ffc107; }
        
