                    <div class="stat-description">+<%= typeof newMembersThisMonth !== 'undefined' ? newMembersThisMonth.toLocaleString('en-PH', { maximumFractionDigits: 0 }) : '0' %> this month</div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-header">
                        <div class="stat-title">Pending Applications</div>
                        <div class="stat-icon rental-icon">üìù</div>
                    </div>
                    <div class="stat-value"><%= typeof pendingApplications !== 'undefined' ? pendingApplications.toLocaleString('en-PH', { maximumFractionDigits: 0 }) : '0' %></div>
                    <div class="stat-description"><%= typeof pendingLoanCount !== 'undefined' ? pendingLoanCount.toLocaleString('en-PH', { maximumFractionDigits: 0 }) + ' loan applications' : '0 loan applications' %></div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-header">
                        <div class="stat-title">Total Savings</div>
                        <div class="stat-icon savings-icon">üíπ</div>
                    </div>
                    <div class="stat-value">‚Ç±<%= typeof totalSavings !== 'undefined' ? totalSavings.toLocaleString('en-PH') : '0' %></div>
                    <div class="stat-description">+‚Ç±<%= typeof savingsIncreaseThisMonth !== 'undefined' ? savingsIncreaseThisMonth.toLocaleString('en-PH') : '0' %> this month</div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-header">
                        <div class="stat-title">Bayanihan Savings</div>
                        <div class="stat-icon bayanihan-icon">ü§ù</div>
                    </div>
