    </style>
</head>
<body>
    <div class="container">
        <%- include('../partials/adminsidebar') %>

        <div class="main-content">
            <div class="dashboard-header">
                <div class="welcome-section">
                    <h1>Welcome to Cooperative Admin</h1>
                    <p>Monitor and manage your cooperative's operations</p>
                </div>
                <div class="date-section">
                    <script>
                        const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };
                        document.write(new Date().toLocaleDateString('en-US', options));
                    </script>
                </div>
            </div>
            
            <div class="stats-container">
                <div class="stat-card">
                    <div class="stat-header">
                        <div class="stat-title">Active Loans</div>
                        <div class="stat-icon loan-icon">üí∞</div>
                    </div>
                    <div class="stat-value">‚Ç±<%= typeof totalActiveLoans !== 'undefined' ? totalActiveLoans.toLocaleString('en-PH') : '0' %></div>
                    <div class="stat-description"><%= typeof activeLoanCount !== 'undefined' ? activeLoanCount.toLocaleString('en-PH', { maximumFractionDigits: 0 }) + ' active loans' : '0 active loans' %></div>
                </div>
                
                <div class="stat-card">
                    <div class="stat-header">
